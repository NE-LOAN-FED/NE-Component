{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports":true,
    "target": "es6",
    "declaration": true,
    "types": [
      "react",
      "react-dom"
    ]
  },
  "exclude": [
    "node_modules",
    "lib",
    "es"
  ]
}
