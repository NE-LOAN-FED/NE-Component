    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
