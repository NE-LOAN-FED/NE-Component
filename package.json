{
  "name": "ne-rc",
  "version": "0.2.0-beta.3",
  "title":"NE-ReactComponents",
  "description": "A react compoents develop by NE-LOAN-FED",
  "homepage":"https://github.com/NE-LOAN-FED/NE-Component",
  "main": "lib/index.js",
  "scripts": {
    "demo": "node ./node_modules/better-npm-run demo",
    "demo:build": "rimraf ./demo && node ./node_modules/better-npm-run demo:build",
    "build:webpack": "node ./node_modules/better-npm-run build:webpack",
    "build:babel": "node ./node_modules/better-npm-run build:babel",
    "build": "npm run clean && npm run build:webpack && npm run build:babel",
    "clean": "rimraf ./dist ./lib",
    "lint": "eslint components examples test && echo \"eslint: no lint errors\"",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "scsslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test --watch"
  },
  "betterScripts": {
    "demo": {
      "command": "webpack-dev-server --config ./config/webpack.config.js",
      "env": {
        "NODE_ENV": "demo"
      }
    },
    "demo:build": {
      "command": "webpack --config ./config/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "demo:build"
      }
    },
    "build:webpack": {
      "command": "webpack --config ./config/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:babel": {
      "command": "babel ./components --out-dir ./lib --copy-files && cp dist/ne.min.css lib/ne.min.css",
      "env": {
        "NODE_ENV": "build"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "css-animation": "^1.3.2",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "rc-animate": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "better-npm-run": "0.0.14",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-react": "^6.2.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.1",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-router": "^3.0.2",
    "require-reload": "^0.2.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0-0 || 15.x",
    "react-dom": "0.14.x || ^15.0.0-0 || 15.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NE-LOAN-FED/NE-Component.git"
  },
  "bugs": {
    "url": "https://github.com/NE-LOAN-FED/NE-Component/issues"
  },
  "keywords": [
    "react",
    "component",
    "react-components",
    "frontend"
  ],
  "author": "NE-LOAN-FED",
  "license": "MIT"
}
