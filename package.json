{
  "name": "ne-rc",
  "version": "0.1.1",
  "description": "A react compoents develop by NE-LOAN-FED",
  "main": "components/index.js",
  "scripts": {
    "demo": "node ./node_modules/better-npm-run demo",
    "build": "node ./node_modules/better-npm-run build",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./dist ./lib",
    "lint": "eslint components examples test && echo \"eslint: no lint errors\"",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "scsslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test --watch"
  },
  "betterScripts": {
    "demo": {
      "command": "webpack-dev-server --config ./config/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "demo"
      }
    },
    "build": {
      "command": "npm run build:clean && webpack --config ./config/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "lodash": "^4.16.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-require-ignore": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.11",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "mocha": "^3.1.2",
    "node-sass": "^3.9.3",
    "postcss-loader": "^0.9.1",
    "rc-queue-anim": "^0.12.5",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "require-reload": "^0.2.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NE-LOAN-FED/NE-Component.git"
  },
  "bugs": {
    "url": "https://github.com/NE-LOAN-FED/NE-Component/issues"
  },
  "keywords": [
