{
  "name": "NE-Component",
  "version": "0.1.1",
  "description": "内部组件",
  "main": "index.js",
  "scripts": {
    "demo": "node ./node_modules/better-npm-run dev",
    "dev": "node ./node_modules/better-npm-run dev",
    "dist": "node ./node_modules/better-npm-run dist",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "scsslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test --watch"
  },
  "betterScripts": {
     "demo": {
      "command": "node ./server.js",
      "env": {
        "NODE_ENV": "demo"
      }
    },
    "dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dist": {
      "command": "node node_modules/rimraf/bin.js rm -rf dist && webpack --config webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "lodash": "^4.16.4"
  },
