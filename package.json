{
  "name": "ne-rc",
  "version": "0.6.1-beta.1",
  "title": "NE-ReactComponents",
  "description": "A react compoents develop by NE-LOAN-FED",
  "homepage": "https://github.com/NE-LOAN-FED/NE-Component",
  "main": "lib/index",
  "module": "es/index",
  "jsnext:main": "es/index",
  "scripts": {
    "clean": "rimraf ./dist ./lib ./es",
    "demo": "cross-env NODE_NEV=demo webpack-dev-server --config ./config/index.js",
    "demo:build": "rimraf ./demo && cross-env NODE_NEV=demo:build webpack --config ./config/index.js --progress --colors",
    "build:umd": "cross-env NODE_ENV=production webpack --config ./config/index.js --progress --colors",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --copy-files && cp dist/ne.min.css lib/ne.min.css",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --copy-files && cp dist/ne.min.css es/ne.min.css",
    "build": "npm run clean && npm run build:umd && npm run build:cjs && npm run build:es",
    "eslint": "node ./node_modules/eslint/bin/eslint.js ./src",
    "scsslint": "node ./node_modules/eslint/bin/eslint.js ./",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test --watch"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "css-animation": "^1.3.2",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "rc-animate": "^2.3.3",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-require-ignore": "^0.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "better-npm-run": "0.0.14",
    "chai": "^3.5.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "exports-loader": "^0.6.4",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.1",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-router": "^3.0.2",
    "require-reload": "^0.2.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0-0 || 15.x",
    "react-dom": "0.14.x || ^15.0.0-0 || 15.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NE-LOAN-FED/NE-Component.git"
  },
  "bugs": {
    "url": "https://github.com/NE-LOAN-FED/NE-Component/issues"
  },
  "keywords": [
    "react",
    "component",
    "react-components",
    "frontend"
  ],
  "author": "NE-LOAN-FED",
  "license": "MIT"
}
