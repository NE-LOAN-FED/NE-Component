    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.11",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.4.0",
