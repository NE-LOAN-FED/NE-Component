{
  "presets": [
    "./tools/es2015Preset",
    "react",
    "stage-2"
  ],
  "env": {
    "cjs": {
      "plugins": [
        "add-module-exports",
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [
              ".scss"
            ]
          }
        ]
      ]
    },
    "es": {
      "plugins": [
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [
              ".scss"
            ]
          }
        ]
      ]
    }
  }
}
